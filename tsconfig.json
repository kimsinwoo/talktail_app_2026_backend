{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "src/services",
    "rootDir": "src/services",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "include": ["src/services/healthAiService.types.ts", "src/services/healthAiService.ts"],
  "exclude": ["node_modules"]
}
